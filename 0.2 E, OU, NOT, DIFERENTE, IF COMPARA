#include <iostream>
using namespace std;

int main ()
{

float a = 10;
float b = 8;
float c = 7;
int d = 10;
    if (a>b && a!=c)
    {
        cout << "a eh maior que b e diferente de c" << endl;
    }

    if (a<b || b>c)
    {
        cout << "a NAO eh maior que b, mas eh MAIOR QUE C - OU - ao CONTRARIO OU - os 2 AO MESMO TEMPO" << endl;
    }

    if (a==d)
    {
        cout << "a = b E ATENCAO: EU NAO USEI O NOT '!' " << endl;

    }
//cout será 1 (9-8, é o resto da divisão por 9)
}

